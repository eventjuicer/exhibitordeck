{
  "expo": {
    "name": "E-commerce Berlin Expo Exhibitor",
    "description" : "Allows exhibitors to scan visitors' badges and fetch their contact details.",
    "slug": "ebe-exhibitor",
    "privacy": "public",
    "owner" : "adam zygadlewicz",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "2.1.5",
    "orientation": "portrait",
    "icon": "./assets/app.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffd700",
      "hideExponentText" : true
    },
    "updates": {
      "enabled" : true,
      "checkAutomatically" : "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 300000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme" : "ebe-exhibitor",
    "primaryColor" : "#8a1719",
    "androidStatusBarColor" : "#8a1719",
    "ios": {
      "bundleIdentifier": "com.eventjuicer.ebe.exhibitors",
      "supportsTablet": true
    },
    "android": {
      "package": "com.eventjuicer.ebe.exhibitors",
      "permissions" : ["CAMERA"]
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "eventjuicer",
            "project": "exhibitordeck",
            "authToken": "a9c5fa18c4244ddc862280272995e8dd053452c8530b45c29e0aa549d7566edc"
          }
        }
      ]
    }
  }
}